package com.anu.fileHandling;

import java.io.File;

public class FileHandling {
public void demo1() {
	try {
        File file = new File("abc.txt");
       if(file.isDirectory()) {
    	   System.out.println("This is a Directory");
       }else {
    	   System.out.println("this is a file");
       }
     } catch(Exception e) {
        e.printStackTrace();
     }
}
}
